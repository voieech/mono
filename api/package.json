{
  "name": "api",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npx tsx ./src/index.ts",
    "serve": "npx tsx watch ./src/index.ts",
    "typecheck": "npx tsc --noEmit",
    "start-new": "node ./dist/index.js",
    "delete-build-cache": "npx rimraf tsconfig.tsbuildinfo dist",
    "build:server": "npx tsc",
    "build": "npm run delete-build-cache && npm run build:server",
    "db:migrate": "npx tsx ./src/kysely/migration-cli/kyselyMigrationCli.ts",
    "db:seed": "npx tsx ./src/kysely/seed/runSeed.ts",
    "lint": "npx eslint",
    "lint:fix": "npx eslint --fix",
    "scratchpad": "npx tsx ./src/scratchpad.ts",
    "scratchpad:watch": "npx tsx watch ./src/scratchpad.ts",
    "reverse-proxy": "npx ngrok http 3000"
  },
  "dependencies": {
    "@workos-inc/node": "^7.71.0",
    "convert-unknown-catch-to-error": "^1.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "datetime-ts": "^1.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "nice-types": "^1.0.1",
    "picocolors": "^1.1.1",
    "rss": "^1.2.2",
    "safety-number-one": "^1.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.2",
    "@types/rss": "^0.0.32",
    "eslint": "^9.29.0",
    "eslint-plugin-perfectionist": "^4.14.0",
    "globals": "^16.2.0",
    "kysely": "^0.28.2",
    "kysely-codegen": "^0.18.5",
    "pg": "^8.16.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
