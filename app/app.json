{
  "expo": {
    "name": "voieech",
    "slug": "voieech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon-dark.png",
    "scheme": "voieech",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.voieech",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "UIBackgroundModes": ["audio"]
      },
      "associatedDomains": ["applinks:voieech.com"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.voieechapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "locales": {
      "zh-Hans": "./app_languages/chinese_simplified.json",
      "zh-Hant": "./app_languages/chinese_traditional.json"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": [
              "en",
              "zh-Hans",
              "zh-Hant"
            ],
            "android": [
              "en",
              "zh-Hans",
              "zh-Hant"
            ]
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "88e4c651-e6f7-40af-8b21-0ac2976bd6f6"
      }
    },
    "owner": "voieech",

    // Get expo to auto calculate runtime version using @expo/fingerprint lib
    "runtimeVersion": {
      "policy": "fingerprint"
    },

    "updates": {
      "url": "https://u.expo.dev/88e4c651-e6f7-40af-8b21-0ac2976bd6f6"
    }
  }
}
